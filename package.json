{
  "name": "orbital-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest  --verbose"
  },
  "jest": {
    "preset": "jest-expo",
    "roots": [
      "."
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-native-safe-area-context|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|expo-router|react-native-screens|expo-constants|expo-status-bar|expo-linking|native-base|react-native-svg|react-native-gesture-handler|react-native-paper|expo-image-picker|react-native-toast-notifications)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupFiles": [
      "./jestSetupFile"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx,tsx,ts}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.16",
    "@supabase/supabase-js": "^2.22.0",
    "expo": "~48.0.15",
    "expo-camera": "~13.2.1",
    "expo-constants": "~14.2.1",
    "expo-device": "~5.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-linking": "~4.0.1",
    "expo-media-library": "~15.2.3",
    "expo-notifications": "~0.18.1",
    "expo-router": "^1.5.3",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "^0.16.4",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "jest-extended": "^4.0.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.3.2",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-switch-selector": "^2.3.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@testing-library/react-native": "^12.1.2",
    "react-native-dotenv": "^3.4.8",
    "redux-mock-store": "^1.5.4"
  },
  "private": true
}
