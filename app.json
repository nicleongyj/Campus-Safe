{
  "expo": {
    "name": "orbital-app",
    "scheme": "orbital-app",
    "slug": "orbital-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CampusSafe.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nickyujun.orbitalapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.nickyujun.orbitalapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3bdb4588-6ab3-431e-bb4c-ca7e78cea575"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3bdb4588-6ab3-431e-bb4c-ca7e78cea575"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CampusSafe to access your camera.",
          "photosPermission": "Allow CampusSafe to access your photos.",
          "savePhotosPermission": "Allow CampusSafe to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
